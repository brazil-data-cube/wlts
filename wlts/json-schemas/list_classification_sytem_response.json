{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "localhost/wlts/list_collection_response.json",
  "type": "object",
  "title": "WLTS - List Classification System operation",
  "description": "Retrieves list of offered classification system",
  "required": [
    "classification_system"
  ],
  "classification_system": {
      "$id": "#/properties/classification_system",
      "type": "array",
      "title": "Classification System Names",
      "description": "Defines list of classification system names from server",
      "default": false,
      "items": {
        "$id": "#/properties/classification_system/items",
        "type": "string",
        "title": "Classification System Identifier",
        "description": "Classification System identifier",
        "default": "",
        "examples": [
          "CORINE Land Cover"
        ]
      }
    }
}