{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "title": "WLTS - Trajectory operation",
  "description": "Retrieves trajectory",
  "readOnly": true,
  "writeOnly": false,
  "required": [
    "latitude",
    "longitude"
  ],
  "properties": {
    "collections": {
      "$id": "#/properties/collections",
      "type": "array",
      "items": {
            "type": "string"
      },
      "title": "Collections Identifier",
      "description": "Collections identifier",
      "default": "",
      "examples": [
        "prodes"
      ]
    },
    "latitude": {
      "$id": "#/properties/latitude",
      "type": "string",
      "title": "Latitude coordinate",
      "description": "Latitude coordinate",
      "pattern": "\\d"
    },
    "longitude": {
      "$id": "#/properties/longitude",
      "type": "string",
      "title": "Longitude coordinate",
      "description": "Longitude coordinate",
      "pattern": "\\d"
    },
    "start_date": {
      "$id": "#/properties/start_date",
      "type": "string",
      "title": "Start date",
      "description": "Start date"
    },
    "end_date": {
      "$id": "#/properties/end_date",
      "type": "string",
      "title": "End date",
      "description": "End date"
    }
  }
}
