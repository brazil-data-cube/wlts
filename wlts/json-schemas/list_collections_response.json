{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "localhost/wlts/list_collection_response.json",
  "type": "object",
  "title": "WLTS - List Collection operation",
  "description": "Retrieves list of offered collections",
  "required": [
    "collections"
  ],
  "properties": {
    "collections": {
      "$id": "#/properties/collections",
      "type": "array",
      "title": "Collections Names",
      "description": "Defines list of collections names from server",
      "default": false,
      "items": {
        "$id": "#/properties/collections/items",
        "type": "string",
        "title": "Collection Identifier",
        "description": "Collection identifier",
        "default": "",
        "examples": [
          "prodes",
          "deter"
        ]
      }
    }
  }
}